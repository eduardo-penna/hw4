<div class="container-fluid">
  <h1>Login</h1>

  <%= form_with :url => "/sessions" do |form| %>
    <div class="mb-3">
      <%= form.label "email" %>
      <%= form.text_field "email" %>
      <%= form.label "password" %>
      <%= form.password_field "password" %>
    </div>
    
    <div class="mb-3">
      <%= form.submit "Login" %>
    </div>
  <% end %>
</div>